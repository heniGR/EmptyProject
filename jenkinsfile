pipeline {
    agent any

    stages {
        stage('Compile Stage') {
        
        	steps{
        	
        		withMaven( : 'M2_HOME'){
        			sh 'mvn clean compile'
        	   }
        	}   
        }
        stage('Testing Stage') {
        
        	steps{
        	
        		withMaven( : 'M2_HOME'){
        			sh 'mvn test'
        	   }
        	}   
        }
       stage('Deployement Stage') {
        
        	steps{
        	
        		withMaven( : 'M2_HOME'){
        			sh 'mvn deploy'
        	   }
        	}   
        }
    }
}