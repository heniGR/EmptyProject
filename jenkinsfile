pipeline {
    agent any

    stages {
       stage('Pulling') {
        
        	steps{
        	
        	
        	git branch: 'master',
        	url : 'https://github.com/heniGR/EmptyProject.git'
        	   
        	}   
        }
        stage('Compile Stage') {
        
        	steps{
        	
        		
        			sh 'mvn clean compile'
        	   
        	}   
        }
      
       stage('Deployement Stage') {
        
        	steps{
        	
        	
        			sh 'mvn deploy'
        	   
        	}   
        }
    }
}